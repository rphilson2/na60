<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Partnering_Bank_Number_Format</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX( Partnering_Bank_Number__c , &quot;([a-z]|[A-Z]|[0-9]){3}-([a-z]|[A-Z]|[0-9]){3}&quot;))</errorConditionFormula>
    <errorDisplayField>Partnering_Bank_Number__c</errorDisplayField>
    <errorMessage>Only numbers formatted as XXX-XXX are allowed.</errorMessage>
</ValidationRule>
