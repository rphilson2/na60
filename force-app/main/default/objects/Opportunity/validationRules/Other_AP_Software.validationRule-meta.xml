<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Other_AP_Software</fullName>
    <active>true</active>
    <errorConditionFormula>AND ( 
ISPICKVAL( AP_Software_Type__c , &quot;Other&quot;), 
ISBLANK(  Other_AP_Software__c  ) 
)</errorConditionFormula>
    <errorDisplayField>Other_AP_Software__c</errorDisplayField>
    <errorMessage>You must fill in the Other AP Software field if &#39;Other is chosen for AP Software Type</errorMessage>
</ValidationRule>
