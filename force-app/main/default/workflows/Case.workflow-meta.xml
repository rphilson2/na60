    <rules>
        <fullName>Set Trigger Criteria</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Case.RecordTypeId</field>
            <operation>equals</operation>
            <value>FCA Card Cases</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.User_Issue_or_Question__c</field>
            <operation>equals</operation>
            <value>Did Not Apply</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Set_Case_Trigger_Criteria</name>
