    <rules>
        <fullName>Change Owner to CTS Support Queue</fullName>
        <actions>
            <name>Change_Owner</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>(1 OR 2) AND 3</booleanFilter>
        <criteriaItems>
            <field>Case.Case_Owner_s_Role__c</field>
            <operation>contains</operation>
            <value>CTS</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Case_Owner_s_Role__c</field>
            <operation>contains</operation>
            <value>Global</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.RecordTypeId</field>
            <operation>equals</operation>
            <value>CTS Support</value>
        </criteriaItems>
        <description>When a CTS RM creates a support case, change the owner from that RM to the CTS Support queue</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Close DNA Cases</fullName>
        <actions>
            <name>Email_Alert_Test_Case_DNA</name>
