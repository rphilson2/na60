        </actions>
        <actions>
            <name>Change_Status_to_Closed1</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>notEqual</operation>
            <value>Closed</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.User_Issue_or_Question__c</field>
            <operation>equals</operation>
            <value>Did Not Apply</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Trigger_Criteria__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
